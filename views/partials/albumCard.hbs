<div class="col-md-4" style="margin-bottom: 20px;">
  <div class="card h-100">
    <img class="card-img-top" src={{#if this.images}}
                {{this.images.[0].url}}
            {{else}}
                "https://nidcap.org/wp-content/uploads/2021/03/book.png"
            {{/if}}/>
    <div class="card-body">
      <h5 class="card-title  d-flex flex-column bd-highlight mb-3">
        {{this.name}}
        <a class="align-self-center" style="padding-top: 20px;" href="/tracks/{{this.id}}"><button class="btn btn-danger">See Tracks</button></a>
      </h5>
    </div>
  </div>
</div>